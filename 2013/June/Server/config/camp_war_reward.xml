  <!--	阵营欢乐斗奖励配置
		gold_base_value绑定金币基准数据（第一名奖励金币值，后面照前）  
		gold_dec_value绑定金币递减大小（每靠后一名减少金币大小，后面照前）
		prestige_base_value声望基准数据
		prestige_dec_value声望递减大小
		rank_limit_value排名递减限制大小(等于或者小于这个名次，奖励不递减，												和rank_limit_value-1名次奖励相同)	-->
  <ordinary_campWar_reward 
	gold_base_value="500000"
	gold_dec_value="50000" 
	prestige_base_value="300" 
	prestige_dec_value="30" 
	rank_limit_value="10"
	/>
 
